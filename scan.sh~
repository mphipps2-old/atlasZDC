#!/bin/bash

rm results/*
rm output/*

 for t in $(seq 0 0); do
	for y in $(seq 0 32); do
    	    for z in $(seq 0 32); do
			./build/zdc $y $z $t run1.mac >> ~/zdc/output/out$t.txt 2>> ~/zdc/output/err$t.txt
#		./build/zdc $y $z 0 run1.mac >> ~/zdc/output/out1.txt >> err1.txt
    	    done
	done
 done
